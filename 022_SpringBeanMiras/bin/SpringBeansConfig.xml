<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	

	<bean id="selam" class="com.furkankara.Selam">
		<property name="mesaj1" value="Hasan Balta"></property>
		<property name="mesaj2" value="Burak Ada"></property>
	</bean>
	<bean id="yazar" class="com.furkankara.Yazar" parent="selam">
		<property name="mesaj1" value="Barış Ataman"></property>
		<property name="mesaj3" value="Mert Bozkurt"></property>
	</bean>
</beans>
